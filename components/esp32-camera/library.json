{
  "name": "ESP32-CAM PSRAMera",
  "version": "1.0.0",
  "keywords": "esp32, camera, espressif, ESP32-CAM PSRAM",
  "description": "ESP32 compatible driver for OV2640, OV3660, OV5640, OV7670 and OV7725 image sensors.",
  "repository": {
    "type": "git",
    "url": "https://github.com/espressif/ESP32-CAM PSRAMera"
  },
  "frameworks": "espidf",
  "platforms": "*",
  "build": {
    "flags": [
      "-Idriver/include",
      "-Iconversions/include",
      "-Idriver/private_include",
      "-Iconversions/private_include",
      "-Isensors/private_include",
      "-fno-rtti"
    ],
    "includeDir": ".",
    "srcDir": ".",
    "srcFilter": ["-<*>", "+<driver>", "+<conversions>", "+<sensors>"]
  }
}
